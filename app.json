{
      "name": "WAIFU-HUSBANDO-CATCHER",
      "description": "Telegram Husbando/Waifu Catcher Bot.",
      "logo": "https://graph.org/file/1b2d3c1491fbaa85a6d29.jpg",
      "keywords": [
         "telegram",
         "modular",
         "Husbando Catcher",
         "Waifu Catcher",
         "Catcher"
      ],   
   "repository": "https://github.com/Pyrogramsupport/Waifu",
   "stack": "heroku-22",
   "addons": [
      {
         "options": {
            "version": "16"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather",
         "required": true,
         "value": ""
      },
      "api_id": {
         "description": "Get API_ID from my.telegram.org .",
         "required": true,
         "value": "19042248"
      },
      "api_hash": {
         "description": "Get API_HASH from my.telegram.org .",
         "required": true,
         "value": "a1d443cb79941a89c493f22abf4f84cb"
      },
      "BOT_USERNAME": {
         "description": "Your Bot Username without @",
         "required": true,
         "value": "Secure_x_robot"
      },
      "mongo_url": {
         "description": "Your Mongo URL.. Don't know how to get Then Search on yt",
         "required": true,
         "value": ""
      },
      "sudo_users": {
         "description": "",
         "required": true,
         "value": "" 
      },
      "SUPPORT_CHAT": {
         "description": "your bots support chat name without @ or chat id.",
         "required": true,
         "value": "secure_chattt" 
      },
      "CHARA_CHANNEL_ID": {
         "description": "A channel id where bot can sends all your uploaded Characters",
         "required": true,
         "value": "-123" 
      },
      "OWNER_ID": {
         "description": "Id of bot owner",
         "required": true,
         "value": "6765826972" 
      },
     "GROUP_ID": {
         "description": "Your Support Group id",
         "required": true,
         "value": "" 
      }
   }     
      
}
